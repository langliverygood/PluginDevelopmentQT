/* 设置 */
MenuButton#setting_btn
{
width:32px;
height:32px;
background:#1c1f29;
border-radius:73px;
border-image:url(":/navigationResources/image/setting_new.png");
border:none;
background:transparent;
}

QMenu
{
background:#2a2929;
border-radius:4px;
padding:4px;
margin:10px;
}

QMenu::item
{
font-family:HarmonyOS Sans SC;
font-size: 16px;
border-radius:4px;
color:rgb(225,225,225);
padding:10px;
margin:5px;
}

QMenu::item:selected
{
background:rgba(82,130,164,1);
}

QMenu::item:pressed
{
background:rgba(82,130,164,0.4);
}

